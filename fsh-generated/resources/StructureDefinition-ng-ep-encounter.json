{
  "resourceType": "StructureDefinition",
  "id": "ng-ep-encounter",
  "url": "https://dhin-hie.org/ePrescription/StructureDefinition/ng-ep-encounter",
  "version": "1.0.0",
  "name": "NgePEncounter",
  "title": "NG-eP Encounter",
  "status": "active",
  "publisher": "DHIN",
  "description": "A profile for encounters in the Nigerian  prescription guide, used to document prescription visits and related healthcare interactions.",
  "fhirVersion": "4.0.1",
  "kind": "resource",
  "abstract": false,
  "type": "Encounter",
  "baseDefinition": "http://hl7.org/fhir/StructureDefinition/Encounter",
  "derivation": "constraint",
  "differential": {
    "element": [
      {
        "id": "Encounter.status",
        "path": "Encounter.status",
        "short": "Status of the prescription encounter"
      },
      {
        "id": "Encounter.subject",
        "path": "Encounter.subject",
        "min": 1,
        "type": [
          {
            "code": "Reference",
            "targetProfile": [
              "https://dhin-hie.org/ePrescription/StructureDefinition/NgePPatient"
            ]
          }
        ],
        "mustSupport": true
      },
      {
        "id": "Encounter.participant",
        "path": "Encounter.participant",
        "min": 1,
        "mustSupport": true
      },
      {
        "id": "Encounter.participant.individual",
        "path": "Encounter.participant.individual",
        "type": [
          {
            "code": "Reference",
            "targetProfile": [
              "https://dhin-hie.org/ePrescription/StructureDefinition/NgePPractitioner"
            ]
          }
        ]
      },
      {
        "id": "Encounter.location",
        "path": "Encounter.location",
        "min": 1,
        "max": "1",
        "mustSupport": true
      },
      {
        "id": "Encounter.location.location",
        "path": "Encounter.location.location",
        "type": [
          {
            "code": "Reference",
            "targetProfile": [
              "https://dhin-hie.org/ePrescription/StructureDefinition/nigeria-location"
            ]
          }
        ]
      },
      {
        "id": "Encounter.serviceProvider",
        "path": "Encounter.serviceProvider",
        "min": 1,
        "type": [
          {
            "code": "Reference",
            "targetProfile": [
              "https://dhin-hie.org/ePrescription/StructureDefinition/ng-ep-organization"
            ]
          }
        ],
        "mustSupport": true
      }
    ]
  }
}
